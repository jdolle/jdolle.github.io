{
  "name": "personal-site",
  "version": "1.0.0",
  "description": "Source code for my personal website",
  "main": "dist/index.html",
  "scripts": {
    "start": "npm run server",
    "build": "webpack",
    "build:prod": "NODE_ENV=production npm run build",
    "server": "webpack-dev-server --content-base dist/",
    "test": "NODE_ENV=test nyc --reporter=lcov mocha $(find src -name '*.spec.js') --require babel-core/register --require babel-polyfill",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./src",
    "deploy": "npm run build && gh-pages -b master -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdolle/jdolle.github.io.git"
  },
  "author": "Jeff Dolle",
  "license": "MIT",
  "homepage": "https://github.com/jdolle/jdolle.github.io#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-babel": "^6.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.4.0",
    "nyc": "^10.1.2",
    "react": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  }
}
